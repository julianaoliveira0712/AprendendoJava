package ExemploInterface;

public class ExemploInterface {

	public static void main(String[] args) {

		Alimento arroz = new Alimento 
				(1234,"arroz - 5kg",15, 4);
		
		Perfume purovoodoo = new Perfume
				(5678,"perfume Rochele",10, "madeirado");
		
		Servico delivery = new Servico
				("Delivery", 7);
		
		Tributo trib = new Tributo();
		trib.adicionaTributavel(arroz);
		trib.adicionaTributavel(purovoodoo);
		trib.adicionaTributavel(delivery);
		
		trib.exibeTributavel();
		
		
		
	}

}
